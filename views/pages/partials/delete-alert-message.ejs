<div class="overlay">
    <div class="overlay-alert-message alert alert-warning alert-dismissible">
        <input type="hidden" class="inputFieldId" value="" />
        <button type="button" class="close close-alert-message"></button>
        <h4 class="message-title"><%= messageTitle %></h4>
        <% if ( messageTitle === "Delete User") { %> 
            <p>
                Are you sure you want to delete the user <span class="strong"></span> ? 
            </p>
        <% } else if (messageTitle === "Delete Page") { %>
            <p>
                Are you sure you want to delete the page <span class="strong"></span> ? 
            </p>
        <% } else if (messageTitle === "Delete PDF") { %>
            <p>
                    Are you sure you want to delete the pdf <span class="strong"></span> ?  
            </p>
        <% } else { %>
            <p>
                Are you sure you want to delete the file <span class="strong"></span> ? 
            </p>
        <% } %>
        <div class="btn-list">
            <button class="btn btn-secondary close-alert-message" type="button">No, thanks</button>
            <button name="delete-this" class="btn btn-success confirm-delete close-alert-message" type="submit">Delete it!</button>
        </div>
    </div>
</div>